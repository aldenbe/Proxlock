<UserControl x:Class="proxLock.Pages.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:proxLock.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300">

    <Grid>

        <CheckBox x:Name="runBox" Content="Run" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top" Checked="runBox_Checked" Unchecked="runBox_Unchecked"/>
        <Button x:Name="settingsButton" Content="Settings" HorizontalAlignment="Right" Margin="0,44,20,0" VerticalAlignment="Top" Click="settingsButton_Click"/>
    </Grid>
</UserControl>
